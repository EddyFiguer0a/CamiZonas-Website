{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/dashboard/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/editar-producto/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/nuevo-producto/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/productos/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/delete-product/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/get\\.product/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/products/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/save-product/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/update-product/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/updload/?$",
			"dest": "_render"
		},
		{
			"src": "^/carrito/?$",
			"dest": "_render"
		},
		{
			"src": "^/catalogo/?$",
			"dest": "_render"
		},
		{
			"src": "^/producto/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}